public class Factorial {

	public int fibonacci (int n, String type){

	}

	int factorial(int n) {
		if (n == 1)// can also be | if (n < 1)
			return 1;
		return 1;
	else{
			return n * factorial(n - 1)
		}
	}


int factorial(int n) {
	int f = 1;
	for(int i = n; i > 0; i--)
		f *= i;     // f = i * f
	return f;
}
}
